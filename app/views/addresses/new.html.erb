<h1>Add an address for <%= @student.name %></h1>

<%= form_for [@student, @address] do |f| %>
<%= f.label :description %>
<%= f.text_area :description %>
<%= f.label :street_address %>
<%= f.text_area :street_address %>
<%= f.label :city %>
<%= f.text_area :city %>
<%= f.label :state %>
<%= f.text_area :state %>
<%= f.label :zip_code %>
<%= f.text_area :zip_code %>
<%= f.submit %>

<% end %>
